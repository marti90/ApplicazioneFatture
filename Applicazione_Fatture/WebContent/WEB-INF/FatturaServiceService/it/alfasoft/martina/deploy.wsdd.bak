<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from FatturaServiceService WSDL service -->

  <service name="FatturaService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://martina.alfasoft.it"/>
      <parameter name="wsdlServiceElement" value="FatturaServiceService"/>
      <parameter name="schemaQualified" value="http://martina.alfasoft.it"/>
      <parameter name="wsdlServicePort" value="FatturaService"/>
      <parameter name="className" value="it.alfasoft.martina.FatturaServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="FatturaService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getFatturaConCodice" qname="operNS:getFatturaConCodice" xmlns:operNS="http://martina.alfasoft.it" returnQName="retNS:getFatturaConCodiceReturn" xmlns:retNS="http://martina.alfasoft.it" returnType="rtns:Fattura" xmlns:rtns="http://martina.alfasoft.it" soapAction="" >
        <parameter qname="pns:codice" xmlns:pns="http://martina.alfasoft.it" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="prendiTutteFatture" qname="operNS:prendiTutteFatture" xmlns:operNS="http://martina.alfasoft.it" returnQName="retNS:prendiTutteFattureReturn" xmlns:retNS="http://martina.alfasoft.it" returnType="rtns:Fattura" xmlns:rtns="http://martina.alfasoft.it" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="prendiTutteFatture getFatturaConCodice"/>

      <typeMapping
        xmlns:ns="http://martina.alfasoft.it"
        qname="ns:Fattura"
        type="java:it.alfasoft.martina.Fattura"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
